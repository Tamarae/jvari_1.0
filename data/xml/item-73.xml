            <item xmlns="http://www.tei-c.org/ns/1.0" n="73" xml:id="item_073" corresp="Marr 51">
                  <lb n="28" ed="28"/>
                  <p>
                     <date type="liturgical" when="--06-04" corresp="#thursday_pentecost" xml:lang="ka">ხუთშაბათსა სულისა წმიდისა მოსლვისასა</date>
                     <term type="service" subtype="liturgy" xml:id="service_liturgy_049">წირვაჲ</term> და
                     <term type="service" subtype="agape" xml:id="service_agape_059">აღაპი</term>
                     <term type="memorial_type" xml:id="memorial_eternal_044">საუკუნოჲ</term>
                     
                     <persName xml:id="pers_chargaz_afaqisdze_jer082" type="main" role="commemorated" ref="#auth_pers_chargaz_afaqisdze" key="ჩარგაზ_აფაქისძე" xml:lang="ka">
                        <forename>ჩარგაზ</forename>
                        <addName type="patronymic" ref="#auth_pers_afaqis">აფაქის ძისაჲ</addName>
                     </persName> და
                     
                     <persName xml:id="pers_nateli_bedanisdaughter_jer083" type="main" role="commemorated" ref="#auth_pers_nateli_bedanisdaughter" key="ნათელი_ბედანისასული" xml:lang="ka">
                        <forename>ნათელისი</forename>
                        <addName type="patronymic" ref="#auth_pers_bedani">ბედანისა ასულისა</addName>
                     </persName>,
                     
                     <lb n="29" ed="29"/>
                     <seg type="formula" subtype="blessing_standard" xml:id="blessing_formula_061">შეუნდვენ ღმერთმან</seg>.
                  </p>
                  
                  <note type="presentation_summary" xml:lang="en">Eternal memorial with liturgy and agape meal for Chargaz Afaqisdze and Nateli, daughter of Bedani, on Thursday of Pentecost</note>
                  <note type="key_figures" xml:lang="en">Chargaz Afaqisdze: Commemorated deceased, son of Afaqis; Nateli Bedanisdaughter: Commemorated female, daughter of Bedani</note>
