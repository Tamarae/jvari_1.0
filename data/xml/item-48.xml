            <item xmlns="http://www.tei-c.org/ns/1.0" n="48" xml:id="item_048" corresp="Marr 35">
                  <lb n="15" ed="15"/>
                  <p>
                     <date type="liturgical" when="--05-03" corresp="#myrrhbearers_sunday" xml:lang="ka">მენელსაცხებლეთა კჳრიაკესა</date>
                     <term type="service" subtype="liturgy" xml:id="service_liturgy_035">წირვაჲ</term> და
                     <term type="service" subtype="agape" xml:id="service_agape_043">აღაპი</term>
                     <term type="memorial_type" xml:id="memorial_eternal_031">საუკუნოჲ</term>
                     
                     <persName xml:id="pers_iob_betlemaysa_jer048" type="main" role="commemorated" ref="#auth_pers_iob_betlemaysa" key="იობ, ბეთლემაჲსა ბიძა" xml:lang="ka">
                        <forename>იობ</forename>
                        <persName ref="#auth_pers_betlemay" key="ბეთლემაჲ">ბეთლემაჲ<lb n="16" ed="16"/>სა</persName>
                        <note type="relationship">ბიძაჲსჲ</note>
                     </persName>,
                     
                     <seg type="formula" subtype="blessing_standard" xml:id="blessing_formula_043">შეუნდვენ ღმერთმან</seg>.
                     
                     <lb n="17" ed="17"/>
                     <term type="service" subtype="requiem" xml:id="service_requiem_011">მწუხრი პანაშჳდი</term>
                     
                     <persName xml:id="pers_ioseb_khaflay_jer048" type="main" role="commemorated" ref="#auth_pers_ioseb_khaflay" key="იოსებ ხაფლაჲ" xml:lang="ka">
                        <forename>იოსებ</forename>
                        <addName type="patronymic" ref="#auth_pers_khaflay">ხაფლაჲსი</addName>
                     </persName>,
                     
                     <seg type="formula" subtype="blessing_standard" xml:id="blessing_formula_044">შეუნდვენ ღმერთმან</seg>.
                  </p>
                  
                  <note type="presentation_summary" xml:lang="en">Myrrhbearers Sunday liturgy and agape memorial for Iob (uncle of Betlemay) and requiem for Ioseb Khaflay</note>
                  <note type="key_figures" xml:lang="en">Iob: Uncle of Betlemay, commemorated with eternal memorial; Ioseb Khaflay: Son of Khaflay, commemorated with sorrowful requiem on Myrrhbearers Sunday</note>
