            <item xmlns="http://www.tei-c.org/ns/1.0" n="72" xml:id="item_072" corresp="Marr 50">
                  <lb n="26" ed="26"/>
                  <p>
                     <date type="liturgical" when="--06-02" corresp="#tuesday_pentecost" xml:lang="ka">სულისა წმიდისა სამშაბათსა</date>
                     <term type="service" subtype="agape" xml:id="service_agape_058">აღაპი</term>
                     <term type="memorial_type" xml:id="memorial_eternal_042">საუკუნო</term>
                     
                     <persName xml:id="pers_ghobiari_gamrekeli_jer081" type="main" role="commemorated" ref="#auth_pers_ghobiari_gamrekeli" key="ღობიარ_გამრეკელი_ერისთავთერისთავი" xml:lang="ka">
                        <roleName type="noble" ref="#auth_role_eristavi_eristavi">ერისთავთ ერისთავისა</roleName>
                        <forename>ღობიარი<lb n="27" ed="27"/>სა</forename>
                        <addName type="patronymic" ref="#auth_pers_gamrek">გამრეკელისა</addName>
                     </persName>,
                     
                     <seg type="formula" subtype="blessing_standard" xml:id="blessing_formula_060">შეუნდვენ ღმერთმან</seg>. და
                     <term type="service" subtype="liturgy" xml:id="service_liturgy_048">წირვა</term>
                     <date type="liturgical" corresp="#same_day">მასვე დღეს</date>
                     <term type="memorial_type" xml:id="memorial_eternal_043">საუკუნო</term>
                     <persName type="same" ref="#pers_ghobiari_gamrekeli_jer081" role="commemorated">
                        <forename>გამრეკლისავე</forename>
                     </persName>.
                  </p>
                  
                  <note type="presentation_summary" xml:lang="en">Double eternal memorial for Eristavi-Eristavi Ghobiari Gamrekeli: eternal agape meal and separate eternal liturgy on Tuesday of Pentecost</note>
                  <note type="key_figures" xml:lang="en">Ghobiari Gamrekeli: High noble with title Eristavi-Eristavi (chief of chiefs), receiving dual memorial services on the same day</note>
