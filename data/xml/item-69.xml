    <text>
        <body>
            <item xmlns="http://www.tei-c.org/ns/1.0" n="69" xml:id="item_069" corresp="Marr 48">
                  <lb n="21" ed="21"/>
                  <p>
                     <date type="liturgical" when="--05-31" corresp="#pentecost_sunday" xml:lang="ka">სულისა წმიდისა მოს<supplied>ლ</supplied>ვისა კჳრიაკესა</date>
                     <term type="service" subtype="liturgy" xml:id="service_liturgy_045">წირვაჲ</term> და
                     <term type="service" subtype="agape" xml:id="service_agape_055">აღაპი</term>
                     <seg type="formula" subtype="royal_invocation" xml:id="royal_formula_001">
                        <expan>ღ<ex>მრ</ex>თივ</expan> გჳრგჳნოს<lb n="22" ed="22"/>ნისა
                     </seg>
                     
                     <persName xml:id="pers_giorgi_king_jer076" type="royal" role="reference" ref="#auth_pers_king_giorgi" key="მეფეთ მეფე გიორგი" xml:lang="ka">
                        <roleName type="royal" ref="#auth_role_king_of_kings">მეფეთ მეფისა დიდისა</roleName>
                        <forename>
                           <expan>გ<ex>იორგ</ex>ის</expan>
                        </forename>
                     </persName>
                     
                     <persName xml:id="pers_mother_king_giorgi_jer077" type="mother" role="commemorated" ref="#auth_pers_giorgi_mother" key="დედა მეფეთ მეფე გიორგისა" xml:lang="ka">
                        <addName type="relationship" ref="#pers_giorgi_king_jer076">დედისა</addName>
                     </persName>
                     <supplied>და</supplied>
                     
                     <persName xml:id="pers_wife_king_giorgi_jer078" type="spouse" role="commemorated" ref="#auth_pers_giorgi_wife" key="ცოლი მეფეთ მეფე გიორგისა" xml:lang="ka">
                        <addName type="relationship" ref="#pers_giorgi_king_jer076">მე<supplied>უ</supplied>ღლი<supplied>სა</supplied> მისსა</addName>
                        <forename>მესაჲ(?)</forename>
                     </persName>,
                     
                     <seg type="formula" subtype="blessing_standard" xml:id="blessing_formula_058">შეუნდვენ ღმერთმან</seg>.
                  </p>
                  
                  <note type="presentation_summary" xml:lang="en">Royal memorial liturgy and agape meal for the mother and wife of the divinely crowned King of Kings Giorgi on Pentecost Sunday</note>
                  <note type="key_figures" xml:lang="en">King of Kings Giorgi: Royal reference point for commemorated family; Mother of King Giorgi: Royal mother commemorated; Wife of King Giorgi: Royal spouse, possibly named Mesa</note>
                  <note type="liturgical_calendar_order">069</note>
                  <note type="item_classification">royal_family_memorial</note>
                  <note type="financial_summary">No amounts specified</note>
               </item>
        </body>
